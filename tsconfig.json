{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "types": [
      "@types/node",
      "@nuxt/types"
    ],
    "paths": { "@/*": [ "./*" ], "~/*": [ "./*" ] },
    "target": "es2018",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
